<UserControl x:Class="BaseOut"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VisualP"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Height="33" Width="193">
    <Canvas>
        <Rectangle x:Name="OutExec" Fill="White" HorizontalAlignment="Left" Height="17" VerticalAlignment="Top" Width="17" Canvas.Left="167" Canvas.Top="8" Visibility="Hidden">
            <Rectangle.Stroke>
                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveBorderColorKey}}"/>
            </Rectangle.Stroke>
        </Rectangle>
        <Label x:Name="OutDescricao" Content="Then" Canvas.Top="3" HorizontalContentAlignment="Right" Canvas.Left="95" Width="72" Foreground="#FFC8C8C8" Background="{x:Null}"/>
        <Ellipse x:Name="OutValue" Fill="White" Height="17" Width="17" Canvas.Left="167" Canvas.Top="8">
            <Ellipse.Stroke>
                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveBorderColorKey}}"/>
            </Ellipse.Stroke>
        </Ellipse>

    </Canvas>
</UserControl>
